{
  "compilerOptions": {
    "target": "ES5",
    "jsx": "react-jsx",
    "lib": ["ES2015", "DOM"],
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "types": ["vite/client"]
  },
  "include": ["src/**/*", "src/**/*.scss", "src/**/*.css"]
}
