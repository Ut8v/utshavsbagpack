{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "lib": ["ES2023", "DOM"],
    "moduleResolution": "nodenext",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true ,
    "types": ["vite/client"],
    "noEmit": true
  },
 "include": ["src/**/*", "src/**/*.scss", "src/**/*.css"]
}

